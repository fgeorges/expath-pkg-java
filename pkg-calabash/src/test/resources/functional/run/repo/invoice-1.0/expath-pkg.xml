<?xml version="1.0" encoding="UTF-8"?>
<package xmlns="http://expath.org/ns/pkg"
         name="http://fgeorges.org/test/calabash/invoice"
         abbrev="invoice"
         version="1.0"
         spec="1.0">

   <title>Test package for the packaging implementation for Calabash</title>

   <xslt>
      <import-uri>http://fgeorges.org/test/invoice.xsl</import-uri>
      <file>invoice.xsl</file>
   </xslt>
   <xquery>
      <namespace>http://fgeorges.org/test/invoice</namespace>
      <file>invoice.xql</file>
   </xquery>
   <xproc>
      <import-uri>http://fgeorges.org/test/invoice.xpl</import-uri>
      <file>invoice.xpl</file>
   </xproc>
   <schematron>
      <import-uri>http://fgeorges.org/test/invoice.sch</import-uri>
      <file>invoice.sch</file>
