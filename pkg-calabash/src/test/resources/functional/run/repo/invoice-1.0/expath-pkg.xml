   <xquery>
      <import-uri>http://fgeorges.org/test/invoice-main.xq</import-uri>
      <file>invoice-main.xq</file>
   </xquery>
   <xsd>
      <namespace>http://fgeorges.org/test/external-invoice</namespace>
      <file>external-invoice.xsd</file>
   </xsd>
   <rng>
      <import-uri>http://fgeorges.org/test/external-invoice.rng</import-uri>
      <file>external-invoice.rng</file>
   </rng>
   <rnc>
      <import-uri>http://fgeorges.org/test/external-invoice.rnc</import-uri>
      <file>external-invoice.rnc</file>
